$("input[type=text]").each(function(){$(this).attr("class",$(this).attr("class")+" text form-control")}),$("textarea").attr("class","form-control"),$("select").attr("class","select form-control"),$(".form-group.pd-checkbox > .form-field-outer > .form-field-inner > span.value > span").attr("class","form-field-inner custom-control custom-checkbox custom-control-inline"),$(".form-group.pd-checkbox > .form-field-outer > .form-field-inner > span.value > span > input").attr("class","custom-control-input"),$(".form-group.pd-checkbox > .form-field-outer > .form-field-inner > span.value > span > label").attr("class","custom-control-label"),$(".form-group.pd-radio > .form-field-outer > .form-field-inner > span.value > span").attr("class","form-field-inner custom-control custom-radio custom-control-inline"),$(".form-group.pd-radio > .form-field-outer > .form-field-inner > span.value > span > input").attr("class","custom-control-input"),$(".form-group.pd-radio > .form-field-outer > .form-field-inner > span.value > span > label").attr("class","custom-control-label"),$().ready(function(){$(".form-field-outer.last_name").append($(".form-field-inner.first_name")),$(".form-group.first_name").remove()}),varJpostal=$(".form-group.zip> label").attr("for"),varState=$(".form-group.state> label").attr("for"),varCity=$(".form-group.city> label").attr("for"),varAddress=$(".form-group.address_one > label").attr("for"),$("#"+varJpostal).jpostal(JSON.parse('{"postcode":["#'+varJpostal+'"],"address":{"#'+varState+'":"%3","#'+varCity+'":"%4","#'+varAddress+'":"%5"}}'));
